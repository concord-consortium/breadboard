package{	import flash.display.MovieClip;	import flash.events.MouseEvent;		public class Match extends MovieClip	{		public var _hitTarget:MovieClip;		public var _startX:Number;		public var _startY:Number;				public function Match()		{			_startX = this.x;			_startY = this.y;			this.buttonMode = true;			this.addEventListener(MouseEvent.MOUSE_DOWN, dragIt);			this.addEventListener(MouseEvent.MOUSE_UP, dropIt);		}				private function dragIt(event:MouseEvent):void		{			this.startDrag();		}				private function dropIt(event:MouseEvent):void		{			this.stopDrag();		}	}}