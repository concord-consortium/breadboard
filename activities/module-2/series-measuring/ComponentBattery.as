package{	import flash.display.MovieClip;		public class ComponentBattery extends MovieClip	{				public var expandOnFocus:ExpandOnFocus;		private var startX:Number;		private var startY:Number;			public function ComponentBattery() {			trace('ENTER ComponentBattery');			//trace('component breadboard ' +stage.this.x+' '+stage.this.y);			expandOnFocus = new ExpandOnFocus(this, 1.5, -60, 0, this.hitArea_mc);	// give breadboard rollover behavior		}				public function setStartX(value:Number) {			this.startX = value;		}		public function setStartY(value:Number) {			this.startY = value;		}			}}